{
   "author": "tobydavies",
   "manifest_version": 2,
   "name": "MetricSearch",
   "permissions": [ "*://*/controller/*" ],
   "background": {
       "matches": ["*://*/controller/*METRIC*"],
       "scripts": [ "background.js" ],
       "persistent": true
   },
   "version": "0.01",
   "web_accessible_resources": [
       "inject.js"
    ]
}
